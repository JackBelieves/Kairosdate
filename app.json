{
  "expo": {
    "name": "Kairos",
    "slug": "kairos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kairos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anilasj.kairos",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kairos.faithspark_app",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "env": {
        "API_URL": "https://kairos-api-265535909839.us-central1.run.app",
        "API_KEY": "SPARK_ALP25"
      },
      "eas": {
        "projectId": "f823f69a-ffc1-4d98-b356-fac43dc07bb1"
      }
    },
    "owner": "anilasj",
    "updates": {
      "url": "https://u.expo.dev/f823f69a-ffc1-4d98-b356-fac43dc07bb1"
    }
  }
}
